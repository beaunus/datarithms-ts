{
  "defaultSeverity": "error",
  "extends": ["tslint-plugin-prettier", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "file-name-casing": [true, "camel-case"],
    "no-unused-variable": true,
    "prefer-const": true,
    "no-shadowed-variable": true,
    "prettier": true,
    "object-literal-sort-keys": true
  },
  "rulesDirectory": []
}
